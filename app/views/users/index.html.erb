<h1 align="center"> All NUSchedule Users</h1>
<h3 align="center">
<%= link_to 'Home', welcome_home_path %>
</h3>

<div align="center">
  <% @users.each do |user| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li><%= link_to gravatar_for(user), user_path(user) %> </li>
          <li class=“article-title”, style="font-size:30px">
          <%= link_to user.username, user_path(user) %>
          </li>
        </div>
      </div>
    </ul>
  <% end %>
</div>

